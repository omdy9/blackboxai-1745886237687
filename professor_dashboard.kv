<ProfessorDashboard>:
    name: "professor_dashboard"

    BoxLayout:
        orientation: "vertical"
        padding: [50, 50, 50, 50]

        MDRaisedButton:
            text: "Start Attendance"
            size_hint: None, None
            size: 200, 50
            pos_hint: {"center_x": 0.5}
            on_release: root.start_attendance(self)

        MDRaisedButton:
            text: "Stop Attendance"
            size_hint: None, None
            size: 200, 50
            pos_hint: {"center_x": 0.5}
            on_release: root.stop_attendance(self)
